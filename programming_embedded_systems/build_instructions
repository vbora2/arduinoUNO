[avr-gcc -c -g -mmcu=atmega328p led.c blink.c
avr-as -O atmega328p.o atmega328p.S 
avr-ld -O blink.exe atmega328p.o led.o blink.o

1) avr-gcc -DF_CPU=20000000UL -mmcu=atmega328p -Wl,-Map,blink.map blink.c -o blink.o 
2)avr-gcc -g -Os -mmcu=atmega328p -c blink.c
	avr-gcc -g -mmcu=atmega328p -o blink.elf blink.o
	avr-objdump -h -S blink.elf > blink.asm

avr-objcopy -j .text -j .data --set-start 0xC00 -O ihex blink.o blink.hex

	(The -j option indicates that we want the information from the .text and .data segment extracted)

avrdude -V -F -c arduino -p m328p 19200 -P COM21 -U flash:w:blink.hex

update binutils (2.25 latest version as of 5/20/15)
./configure --target=avr --enable-languages="c c++" --prefix=/usr/local/avr
	install everything into /usr/local/avr

PATH="/opt/rtems-4.10/bin:/usr/p4v-2013.1.611503/bin:/usr/local/sbin:/usr/local/avr/bin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/games"

